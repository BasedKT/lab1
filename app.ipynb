   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "def grad_desc_with_const_lr(grad, lr, epoch, x):\n",
    "    points = np.zeros((epoch, 2))\n",
    "    points[0] = x\n",
    "\n",
    "    for i in range(1, epoch):\n",
    "        x = x - lr * np.array(grad(x[0], x[1]))\n",
    "        points[i] = x\n",
    "\n",
    "    return points\n"
   ]
